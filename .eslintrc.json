{
    "parser": "babel-eslint",
    "rules": {
        "strict": 0,
        "quotes": [2, "single"],
        "no-underscore-dangle": 0,
        "no-new": 0,
        "eol-last": 0,
        "new-cap": [1, {
            "capIsNewExceptions": ["Deferred"]
        }],
        "indent": [2, 4, { "SwitchCase": 1 }],
        "react/jsx-quotes": 0,
        "object-shorthand": 1,
        "no-var": 2,
        "prefer-const": 2,
        "no-const-assign": 2,
        "no-unused-vars": 2,
        "max-len": [1, 124],
        "prefer-template": 1
    },
    "env": {
        "node": true,
        "browser": true,
        "jasmine": true,
        "jquery": true,
        "es6": true
    },
    "globals": {
        "module": true,
        "define": true,
        "requirejs": true,
        "require": true
    }
}
